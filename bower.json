{
  "name": "Kiss-Webapp",
  "version": "0.1.0",
  "description": "Kiss-Webapp is a very simple frontend engine for creating mobile webapps with Cordova or PhoneGap. It looks like a light AngularJS but his princpal goal is to have no complex mechanism.",
  "homepage": "https://github.com/CodeCorico/Kiss-Webapp",
  "authors": [
    "CodeCorico <hello@codecorico.com>"
  ],
  "main": "kiss-webapp-full.min.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "webapp",
    "single-page",
    "databinding",
    "virtualization",
    "templates"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "src"
  ],
  "dependencies": {
    "events-manager": "~1.0.4"
  },
  "devDependencies": {
    "jquery-2.1.3.min": "http://code.jquery.com/jquery-2.1.3.min.js"
  }
}
